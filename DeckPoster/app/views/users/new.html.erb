<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <div>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </div>
  <div>
  <%= f.label :email%><br>
  <%= f.text_field :email%><br>
  </div>
  <div>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  </div>
  <%= f.submit %>
<%end%>